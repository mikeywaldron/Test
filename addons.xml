<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<addons>
<addon id="plugin.video.stalker" name="IPTV Stalker" version="1.2.0" provider-name="esxbr">
    <requires>
		<import addon="xbmc.python" version="2.14.0"/>
	</requires>
	<extension point="xbmc.python.pluginsource" library="addon.py">
		<provides>video</provides>
	</extension>
	<extension point="xbmc.service" library="server.py" start="startup" />
	<extension point="xbmc.addon.metadata">
		<platform>all</platform>
		<summary lang="en">IPTV Stalker</summary>
		<description lang="en">IPTV Stalker</description>
		<license>GNU General Public License, v2</license>
		<language></language>
		<forum>http://iptvtalk.org/showthread.php?2514-Kodi-Stalker-video-add-on-all-platforms</forum>
		<source>https://github.com/esxbr/plugin.video.stalker</source>
		<email>patrick.cabral@gmail.com</email>
	</extension>
</addon>

<addon id="repository.Mikeys" name="Mikeys-Repo" version="1.0" provider-name="MW">
	<extension point="xbmc.addon.repository" name="Mikeys Repository">
		<info compressed="false">https://raw.githubusercontent.com/mikeywaldron/Test/master/addons.xml</info>
		<checksum>https://raw.githubusercontent.com/mikeywaldron/Test/master/addons.xml.md5</checksum>
		<datadir zip="true">https://github.com/mikeywaldron/Test/tree/master/zips</datadir>
	</extension>
	<extension point="xbmc.addon.metadata">
		<summary>Top Source For All Your Needs</summary>
		<description>Hosting diffrent add-ons for every need</description>
		<platform>all</platform>
	</extension>
</addon>

<addon id="script.tvguidetecbox" name="iVue TV Guide" version="6.0.23" provider-name="TECBOX DreamHD">
	<requires>
		<import addon="xbmc.python" version="2.1.0"/>
		<import addon="script.module.simplejson" version="2.0.10"/>
	</requires>
	<extension point="xbmc.python.script" library="addon.py"/>
    <extension point="xbmc.service" library="service.py" start="login"/>
	<extension point="xbmc.addon.metadata">
		<description lang="en">iVue Guide allows you to combine some of your favourite live TV plugins for use with a fully working EPG</description>
		<disclaimer lang="en">iVue Guide is based on FTV Guide by rayw1986 and Bluezed, which in turn is based on the original TV Guide addon by twinther.</disclaimer>
		<language></language>
		<platform>all</platform>
		<license>GPLv2</license>
		<forum></forum>
		<website></website>
		<email></email>
		<source></source>
	</extension>
</addon>
</addons>
